			"required": [ "@type", "@id", "ceterms:ctid", "ceterms:name", "ceterms:description", "ceterms:inLanguage", "ceterms:ownedBy", "ceterms:subjectWebpage" ],
			"additionalProperties": false
		},
		"ceterms:PathwayComponent": {
			"type": "object",
