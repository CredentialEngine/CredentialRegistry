		"@context": {
			"type": "string",
			"enum": [ "http://credreg.net/ctdl/schema/context/json" ]
		},
		"@id": { "type": "string" },
		"@id_multi": {
			"type": "array",
			"items": { "$ref": "#/definitions/@id" }
		},
		"@id_absolute": {
			"type": "string",
			"format": "uri"
		},
		"@id_absolute_multi": {
			"type": "array",
			"items": { "$ref": "#/definitions/@id_absolute" }
		},
		"meta:string": { "type": "string" },
		"meta:string_multi": {
			"type": "array",
			"items": { "$ref": "#/definitions/meta:string" }
		},
		"meta:langstring": {
			"type": "object",
			"properties": {
				"en": { "type": "string" },
				"en-US": { "type": "string" },
				"en-GB": { "type": "string" },
				"es": { "type": "string" },
				"pl": { "type": "string" },
				"zh": { "type": "string" },
				"de": { "type": "string" },
				"ru": { "type": "string" }
			}
		},
		"meta:langstring_multi": {
			"type": "array",
			"items": { "$ref": "#/definitions/meta:langstring" }
		},
		"meta:literal_object": {
			"type": "object",
			"properties": {
				"@id": { "$ref": "#/definitions/@id" },
				"@value": { "$ref": "#/definitions/meta:string" },
				"@language": { "$ref": "#/definitions/meta:string" }
			}
		},
		"meta:literal": {
			"anyOf": [
				{ "$ref": "#/definitions/meta:string" },
				{ "$ref": "#/definitions/meta:literal_object" }
			]
		},
		"meta:literal_multi": {
			"type": "array",
			"items": { "$ref": "#/definitions/meta:literal" }
		},
		"ceterms:Pathway": {
			"type": "object",
			"properties": {
				"@type": { "enum": [ "ceterms:Pathway" ] },
